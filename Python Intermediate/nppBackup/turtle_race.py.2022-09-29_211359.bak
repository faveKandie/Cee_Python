import turtle
from turtle import Turtle, Screen, colormode
turtle.colormode(255)

var = Turtle()
rad = Turtle()
edd = Turtle()
read = Turtle()
sod = Turtle()

def state(get):
	color_list = [(95, 158, 160), (100, 149, 237), (176, 196, 222), (147, 112, 219), (255, 192, 203), (255, 218, 185), (255, 105, 180), (176, 224, 230), (135, 206, 250), (255, 20, 147), (221, 160, 221)]
	for n in range(5):
		get = Turtle()
		get.shape("turtle")
		get.penup()
		get.color(colour_list[n])
		get.right(n * 90)
		get.forward(70)
		get.setheading(0)
	print(get)

state(var)
state(rad)
state(edd)
state(read)
state(sod)
