import turtle
from turtle import Turtle, Screen
screen = Screen()
screen.setup(width = 900, height = 900)
wall = Turtle()
wall.color("white")
wall.shape("turtle")
wall.pencolor("deep pink")
wall.pensize(15)
wall.penup()
wall.backward(440)
wall.setheading(270)
wall.pendown()
wall.forward(430)
wall.setheading(0)
wall.forward(870)
wall.setheading(90)
wall.forward(870)
wall.setheading(180)
wall.forward(870)
wall.setheading(270)
wall.forward(380)
wall.penup()
wall.setheading(0)
wall.forward(70)
print(wall)