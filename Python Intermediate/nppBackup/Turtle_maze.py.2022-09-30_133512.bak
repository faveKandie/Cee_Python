import turtle
import random
from turtle import Turtle, Screen
screen = Screen()
screen.setup(width = 900, height = 900)
wall = Turtle()
wall.color("white")
wall.shape("turtle")
wall.pencolor("deep pink")
wall.pensize(15)
wall.penup()
wall.backward(440)
wall.setheading(270)
wall.pendown()
wall.forward(430)
wall.setheading(0)
wall.forward(870)
wall.setheading(90)
wall.forward(870)
wall.setheading(180)
wall.forward(870)
wall.setheading(270)
wall.forward(380)
wall.penup()
wall.setheading(0)
wall.forward(70)
wall.pendown()
wall.setheading(90)
wall.forward(320)

num = [0, 90, 180, 270, 360]
rad = [70, 100, 130, 150]
for n in range(50):
	wall.right(random.choice(num))
	wall.pendown
	wall.forward(random.choice(rad))
	wall.penup()
	wall.right(random.choice(num))
	wall.forward(random.choice(rad))
	wall.pendown()