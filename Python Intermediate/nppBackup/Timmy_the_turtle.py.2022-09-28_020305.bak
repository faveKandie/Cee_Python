cee = Turtle()
cee.shape("turtle")
cee.color("violet")

colours = ["pale violet red", "hot pink", "deep pink", "cornflower blue", "steel blue", "light sky blue", "violet"]
for n in range(3, 10, 1):
	cee.color(random.choice(colours))
	for _ in range(n):
		angle = (360 / n)
		cee.forward(100)
		cee.right(angle)
cee.forward(100)

